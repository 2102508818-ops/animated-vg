{
  "name": "animated-vg",
  "version": "0.0.1",
  "description": "Animated SVG Editor",
  "productName": "AnimatedVG",
  "author": "Krushn Dayshmookh <krushndayshmookh@gmail.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "vitest run",
    "test:watch": "vitest",
    "dev": "quasar dev",
    "build": "quasar build",
    "e2e": "playwright test",
    "e2e:install": "playwright install --with-deps",
    "test:rust": "cargo test --manifest-path src-tauri/Cargo.toml",
    "electron:smoke": "node scripts/electron-smoke.mjs",
    "postinstall": "quasar prepare"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "@tauri-apps/api": "^2.0.1",
    "axios": "^1.2.1",
    "dompurify": "^3.2.6",
    "pinia": "^3.0.1",
    "quasar": "^2.16.0",
    "vue": "^3.4.18",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@electron/packager": "^18.4.2",
    "@eslint/js": "^9.14.0",
    "@playwright/test": "^1.47.0",
    "@quasar/app-vite": "^2.1.0",
    "@tauri-apps/cli": "^2.8.1",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/test-utils": "^2.4.5",
    "autoprefixer": "^10.4.2",
    "electron": "^37.3.1",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "globals": "^15.12.0",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.14",
    "prettier": "^3.3.3",
    "vite-plugin-checker": "^0.9.0",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
